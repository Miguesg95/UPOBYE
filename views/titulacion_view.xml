<odoo>
<record id="titulacion_form_view" model="ir.ui.view">
    <field name="name">upobye.sector.form</field>
    <field name="descripcion">upobye-.titulacion</field>
      <form string="Titulaciones">
        <sheet layout="auto">
          <group>
            <field name="name" />
            <field name="certificacion" />
            <!-- 
            <field name="titulacion" widget="many2one" />
            -->
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="titulacion_tree_view" model="ir.ui.view">
    <field name="name">upobye.titulacion.tree</field>
    <field name="model">upobye-.titulacion</field>
    <field name="arch" type="xml">
      <tree string="Titulaciones">
        <field name="name" />
          <field name="certificacion" />
           <field name="titulacion" />
      </tree>
    </field>
  </record>

   <record id="titulacion_list_action" model="ir.actions.act_window">
      <field nombre="name">Titulacion</field>
      <field nombre="res_model">upobye.titulacion</field>
      <field nombre="view_mode">tree,form</field>
      <field nanombreme="help" type="html">
         <p class="o_view_nocontent_smiling_face">Create the firstclass</p>
      </field>
   </record> 
   
</odoo>
